<head>
    <meta charset="UTF-8">
    <link rel="stylesheet" href="/principal.css">
    <link rel="stylesheet" href="/questionarios.css">
    <title>Bolsonaro 2022</title>
</head>
<body>
    <div id="volumes">
        <audio id="som" src="/audio/soltaosomdj.mp3" autoplay loop></audio>
        <button id="musica"><img src="/imagens/som.png"></button>
        <button id="musicaOff"><img src="/imagens/mute.png"></button>
    </div>
    
    <h1>Questionários</h1>

    <section class="adicionarPerguntas">
        {{#each questionarios}}
            <article class="divQuestionario">
                <input type="text" name="identificação" hidden  id="id{{@index}}" value="{{_id}}" >
                <h1>{{this.titulo}}</h1>
                <p>Criado em: {{this.data}}</p>
                <a href="/questionarios/fazer/{{_id}}">Entrar no questionario</a>
                <p>Clique para entrar no questionário!</p>
            </article>
        {{else}}
            <p class="alertaErro">Não existem questionarios cadastrados</p>
        {{/each}}
    </section>
    <script src="/teste.js"></script>
    <script src="/music.js"></script>
</bpdy>