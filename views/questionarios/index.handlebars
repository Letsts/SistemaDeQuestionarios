<head>
    <meta charset="UTF-8">
    <link rel="icon" href="/imagens/favicon.png">
    <link rel="stylesheet" href="/principal.css">
    <link rel="stylesheet" href="/questionarios.css">
    <title>Questionários</title>
</head>
<body>
    <div id="botoesParticEMusica">
        <audio id="som" src="/audio/soltaosomdj.mp3" loop></audio>
        <button id="musica" class="botoesMenu"><img src="/imagens/mute.png"></button>
        
    </div> 
    <h1>Questionários</h1>
    <section class="adicionarPerguntas">
        
        {{#each questionarios}}
            <article class="divQuestionario">
                <input type="text" name="identificação" hidden  id="id{{@index}}" value="{{_id}}" >
                <h1>{{this.titulo}}</h1>
                <p>Criado em: {{this.data}}</p>
                
                <a href="/questionarios/fazer/{{_id}}">Entrar no questionario</a><br><br>
        
                <span data-id = "{{_id}}" class="salaSincrona">Criar sala Sincrona</span>
                
            </article>
        {{else}}
            <p class="alertaErro">Não existem questionarios cadastrados</p>
        {{/each}}
    </section>
    <script src="/teste.js"></script>
    <script src="/music.js"></script>

    <a id="voltar-para-topo" href="#top">↑</a>

</body>